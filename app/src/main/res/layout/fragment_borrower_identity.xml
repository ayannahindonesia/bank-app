<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@color/graybackground"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:id="@+id/scrollview"
        android:layout_above="@id/buttonNext"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <View
                android:id="@+id/indicator"
                android:layout_marginStart="25dp"
                android:layout_marginEnd="25dp"
                android:layout_marginTop="35dp"
                android:background="@color/darkgreybackground"
                android:layout_width="match_parent"
                android:layout_height="15dp"/>

            <View
                android:id="@+id/indicator2"
                android:layout_marginStart="25dp"
                android:layout_marginEnd="25dp"
                android:layout_marginTop="35dp"
                android:background="@color/colorPrimaryDark"
                android:layout_width="200dp"
                android:layout_height="15dp"/>

            <TextView
                android:id="@+id/dataPemohon"
                android:text="Data Pemohon"
                android:textAlignment="center"
                android:layout_below="@+id/indicator"
                android:layout_marginTop="@dimen/dp10"
                android:textColor="@android:color/black"
                android:textSize="@dimen/fontNormal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <LinearLayout
                android:id="@+id/container1"
                android:orientation="vertical"
                android:layout_marginTop="@dimen/dp20"
                android:layout_marginStart="@dimen/dp20"
                android:layout_marginEnd="@dimen/dp20"
                android:layout_below="@+id/dataPemohon"
                android:padding="@dimen/dp16"
                android:background="@android:color/white"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:text="Nama (sesuai KTP)"
                    android:textSize="@dimen/fontLarge"
                    android:textColor="@android:color/black"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
                <EditText
                    android:hint="Adrian Lorenzo"
                    android:inputType="textPersonName"
                    android:paddingStart="@dimen/dp12"
                    android:paddingEnd="@dimen/dp12"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/fontNormal"
                    android:imeOptions="actionNext"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:ignore="Autofill" />

                <TextView
                    android:text="Jenis kelamin"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/fontLarge"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
                <RadioGroup
                    android:id="@+id/spJenisKelamin"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <RadioButton
                        android:id="@+id/rbPria"
                        android:text="Laki-laki"
                        android:textSize="@dimen/fontNormal"
                        android:textColor="@color/darkgreybackground"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />

                    <RadioButton
                        android:id="@+id/rbPerempuan"
                        android:text="Perempuan"
                        android:textSize="@dimen/fontNormal"
                        android:textColor="@color/darkgreybackground"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />



                </RadioGroup>

                <TextView
                    android:text="No.KTP"
                    android:textSize="@dimen/fontLarge"
                    android:textColor="@android:color/black"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
                <EditText
                    android:hint="1234567890123456"
                    android:inputType="number"
                    android:paddingStart="@dimen/dp12"
                    android:paddingEnd="@dimen/dp12"
                    android:imeOptions="actionDone"
                    android:textSize="@dimen/fontNormal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:ignore="Autofill" />

                <TextView
                    android:text="Tanggal Lahir"
                    android:layout_marginTop="10dp"
                    android:textSize="@dimen/fontLarge"
                    android:textColor="@android:color/black"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />

                <TextView
                    android:id="@+id/dateBirth"
                    android:drawablePadding="5dp"
                    android:gravity="center_vertical"
                    android:drawableStart="@drawable/ic_calendar"
                    android:text="dd-mm-yyyy"
                    android:textSize="@dimen/fontNormal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:ignore="Autofill" />

                <TextView
                    android:text="Tempat lahir"
                    android:textSize="@dimen/fontLarge"
                    android:textColor="@android:color/black"
                    android:layout_marginTop="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
                <EditText
                    android:hint="Jakarta"
                    android:inputType="textPersonName"
                    android:paddingStart="@dimen/dp12"
                    android:paddingEnd="@dimen/dp12"
                    android:imeOptions="actionNext"
                    android:textSize="@dimen/fontNormal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:ignore="Autofill" />

                <TextView
                    android:text="Pendidikan terakhir"
                    android:textSize="@dimen/fontLarge"
                    android:textColor="@android:color/black"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
                <!--<EditText-->
                    <!--android:hint="College level"-->
                    <!--android:inputType="textPersonName"-->
                    <!--android:paddingStart="@dimen/dp12"-->
                    <!--android:paddingEnd="@dimen/dp12"-->
                    <!--android:imeOptions="actionNext"-->
                    <!--android:textSize="@dimen/fontNormal"-->
                    <!--android:layout_width="match_parent"-->
                    <!--android:layout_height="wrap_content"-->
                    <!--tools:ignore="Autofill" />-->
                <Spinner
                    android:id="@+id/spCollageLevel"
                    android:inputType="textPersonName"
                    android:imeOptions="actionNext"
                    android:textSize="@dimen/fontNormal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:ignore="Autofill" />

                <TextView
                    android:text="Nama Ibu kandung"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/fontLarge"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
                <EditText
                    android:hint="Maria Dela Cruz"
                    android:inputType="textPersonName"
                    android:paddingStart="@dimen/dp12"
                    android:paddingEnd="@dimen/dp12"
                    android:imeOptions="actionNext"
                    android:textSize="@dimen/fontNormal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:ignore="Autofill" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/container2"
                android:orientation="vertical"
                android:layout_marginTop="@dimen/dp20"
                android:layout_marginStart="@dimen/dp20"
                android:layout_marginEnd="@dimen/dp20"
                android:layout_below="@+id/container1"
                android:padding="@dimen/dp16"
                android:background="@android:color/white"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:text="Status perkawinan"
                    android:textSize="@dimen/fontLarge"
                    android:textColor="@android:color/black"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
                <Spinner
                    android:id="@+id/spPerkawinan"
                    android:inputType="textPersonName"
                    android:textSize="@dimen/fontNormal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:ignore="Autofill" />

                <TextView
                    android:text="Nama Pasangan"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/fontLarge"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
                <EditText
                    android:hint="Maria Dela Cruz"
                    android:inputType="textPersonName"
                    android:paddingStart="@dimen/dp12"
                    android:paddingEnd="@dimen/dp12"
                    android:imeOptions="actionNext"
                    android:textSize="@dimen/fontNormal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:ignore="Autofill" />

                <TextView
                    android:text="Tanggal Lahir Pasangan"
                    android:layout_marginTop="10dp"
                    android:textSize="@dimen/fontLarge"
                    android:textColor="@android:color/black"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />

                <TextView
                    android:id="@+id/dateBirthSpouse"
                    android:text="dd-mm-yyyy"
                    android:drawablePadding="5dp"
                    android:gravity="center_vertical"
                    android:drawableStart="@drawable/ic_calendar"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/fontNormal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:ignore="Autofill" />

                <TextView
                    android:text="Pendidikan"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/fontLarge"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
                <Spinner
                    android:id="@+id/spPendidikan"
                    android:textSize="@dimen/fontNormal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:ignore="Autofill" />

                <TextView
                    android:text="Jumlah tanggungan (orang)"
                    android:textColor="@android:color/black"
                    android:layout_marginTop="15dp"
                    android:textSize="@dimen/fontLarge"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
                <Spinner
                    android:id="@+id/spTanggungan"
                    android:inputType="number"
                    android:imeOptions="actionNext"
                    android:textSize="@dimen/fontNormal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:ignore="Autofill" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/container3"
                android:orientation="vertical"
                android:layout_marginTop="@dimen/dp20"
                android:layout_marginStart="@dimen/dp20"
                android:layout_marginEnd="@dimen/dp20"
                android:layout_below="@+id/container2"
                android:padding="@dimen/dp16"
                android:background="@android:color/white"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:text="Data Alamat Pemohon"
                    android:textSize="@dimen/fontSuperLarge"
                    android:textStyle="bold"
                    android:textColor="@android:color/black"
                    android:layout_marginBottom="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />


                <TextView
                    android:text="Alamat domisili"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/fontLarge"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
                <EditText
                    android:inputType="textMultiLine"
                    android:lines="2"
                    android:hint="Tulis disini..."
                    android:gravity="top"
                    android:paddingStart="@dimen/dp12"
                    android:paddingEnd="@dimen/dp12"
                    android:imeOptions="actionNext"
                    android:textSize="@dimen/fontNormal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:ignore="Autofill" />

                <TextView
                    android:text="Provinsi"
                    android:layout_marginTop="15dp"
                    android:textSize="@dimen/fontLarge"
                    android:textColor="@android:color/black"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
                <Spinner
                    android:id="@+id/spProvinsi"
                    android:imeOptions="actionNext"
                    android:textSize="@dimen/fontNormal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:ignore="Autofill" />

                <TextView
                    android:text="Kota"
                    android:layout_marginTop="15dp"
                    android:textSize="@dimen/fontLarge"
                    android:textColor="@android:color/black"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
                <Spinner
                    android:id="@+id/spKota"
                    android:textSize="@dimen/fontNormal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:ignore="Autofill" />

                <TextView
                    android:text="Kecamatan"
                    android:textColor="@android:color/black"
                    android:layout_marginTop="15dp"
                    android:textSize="@dimen/fontLarge"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
                <Spinner
                    android:id="@+id/spKecamatan"
                    android:textSize="@dimen/fontNormal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:ignore="Autofill" />

                <TextView
                    android:text="Kelurahan"
                    android:layout_marginTop="15dp"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/fontLarge"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
                <Spinner
                    android:id="@+id/spKelurahan"
                    android:imeOptions="actionNext"
                    android:textSize="@dimen/fontNormal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:ignore="Autofill" />

                <LinearLayout
                    android:orientation="horizontal"
                    android:weightSum="2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:text="RT"
                        android:textColor="@android:color/black"
                        android:layout_weight="1"
                        android:textSize="@dimen/fontLarge"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />

                    <TextView
                        android:text="RW"
                        android:textColor="@android:color/black"
                        android:layout_weight="1"
                        android:textSize="@dimen/fontLarge"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />



                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:weightSum="2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <EditText
                        android:id="@+id/rt"
                        android:hint="001"
                        android:inputType="number"
                        android:layout_weight="1"
                        android:textSize="@dimen/fontNormal"
                        android:imeOptions="actionNext"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />

                    <EditText
                        android:id="@+id/rw"
                        android:hint="001"
                        android:inputType="number"
                        android:layout_weight="1"
                        android:textSize="@dimen/fontNormal"
                        android:imeOptions="actionNext"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />



                </LinearLayout>

                <TextView
                    android:text="Telp Rumah"
                    android:textColor="@android:color/black"
                    android:layout_marginTop="15dp"
                    android:textSize="@dimen/fontLarge"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
                <EditText
                    android:inputType="phone"
                    android:paddingStart="@dimen/dp12"
                    android:paddingEnd="@dimen/dp12"
                    android:imeOptions="actionNext"
                    android:hint="02112345678"
                    android:textSize="@dimen/fontNormal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:ignore="Autofill" />

                <TextView
                    android:text="Lama menempati rumah"
                    android:textColor="@android:color/black"
                    android:layout_marginTop="15dp"
                    android:textSize="@dimen/fontLarge"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <EditText
                        android:inputType="number"
                        android:hint="0"
                        android:paddingStart="@dimen/dp12"
                        android:paddingEnd="@dimen/dp12"
                        android:imeOptions="actionNext"
                        android:textSize="@dimen/fontNormal"
                        android:layout_width="60dp"
                        android:layout_height="wrap_content"
                        tools:ignore="Autofill" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="6dp"
                        android:layout_gravity="center_vertical"
                        android:text="Tahun"
                        android:textColor="@android:color/black"
                        android:textSize="@dimen/fontLarge" />

                </LinearLayout>

                <TextView
                    android:text="Status tempat tinggal"
                    android:layout_marginTop="15dp"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/fontLarge"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
                <Spinner
                    android:id="@+id/spStatusHome"
                    android:imeOptions="actionNext"
                    android:textSize="@dimen/fontNormal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    tools:ignore="Autofill" />

            </LinearLayout>

